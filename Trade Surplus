#include <stdio.h>
int main(void) 
{

	int t;
	scanf("%d",&t);
	while(t--){
	    int a1,a2,b1,b2,c,d,e;
	    scanf("%d %d %d %d",&a1,&a2,&b1,&b2);
	    c=a1-a2;
	    d=b1-b2;
	    e=c+d;
	    if(e<0)
		{
	        printf("YES\n");
	    }
	    else
		{
	        printf("NO\n");
	    }
	    }
	return 0;
}
